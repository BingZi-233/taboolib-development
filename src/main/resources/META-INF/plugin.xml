<idea-plugin>
    <id>org.tabooproject.intellij</id>

    <name>Taboolib Integration</name>
    <vendor>TabooLib</vendor>

    <description><![CDATA[
        The quick fox jumps over the lazy dog, template for Taboolib plugin.
    ]]></description>


    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <moduleBuilder id="TABOO_MODULE" builderClass="org.tabooproject.intellij.ModuleBuilder"/>
    </extensions>
</idea-plugin>